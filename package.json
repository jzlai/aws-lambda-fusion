{
  "name": "aws-lambda-fusion",
  "version": "0.0.1",
  "description": "Runtime data based framework for function fusion",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "standardx **/*.ts",
    "prepublish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jzlai/aws-lambda-fusion.git"
  },
  "author": "Jun-Zhe Lai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jzlai/aws-lambda-fusion/issues"
  },
  "homepage": "https://github.com/jzlai/aws-lambda-fusion#readme",
  "devDependencies": {
    "@types/aws-lambda": "8.10.44",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "standardx": "5.0.0",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "aws-sdk": "2.585.0"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
