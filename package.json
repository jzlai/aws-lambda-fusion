  "bugs": {
    "url": "https://github.com/jzlai/aws-lambda-fusion/issues"
  },
  "homepage": "https://github.com/jzlai/aws-lambda-fusion#readme",
  "devDependencies": {
    "@types/aws-lambda": "8.10.44",
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^12.12.29",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
